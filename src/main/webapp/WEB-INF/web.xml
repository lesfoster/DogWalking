<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp">
	<display-name>Dogs</display-name>
	<servlet>
		<servlet-name>Action</servlet-name>
		<display-name>Action</display-name>
		<servlet-class>servlet.Action</servlet-class>
		<load-on-startup>5</load-on-startup>
	</servlet>
	<servlet>
                <servlet-name>Dogs</servlet-name>
                <display-name>Dogs</display-name>
                <jsp-file>/jsp/Dogs.jsp</jsp-file>
    </servlet>
	<servlet-mapping>
		<servlet-name>Action</servlet-name>
		<url-pattern>/Action</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Action</servlet-name>
		<url-pattern>/start</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Action</servlet-name>
		<url-pattern>/index.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Action</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Action</welcome-file>
		<welcome-file>jsp/Dogs.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

</web-app>
